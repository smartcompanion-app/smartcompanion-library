t, h } from \"@stencil/core\";\n\n@Component({\n  tag: 'sc-station-icon',\n  styleUrl: 'station-icon.scss',\n  shadow: true,\n})\nexport class StationIcon {\n\n  @Prop() upperLimitPercent: number = 97;\n\n  /**\n   * Toggle collected icon\n   */\n  @Prop() collected: boolean = false;\n\n  /**\n   * Percentage value between 0 and 100 to display status\n   */\n  @Prop() collectedPercent: number = 0;\n\n  calculatePercent() {\n    if (this.collectedPercent >= this.upperLimitPercent) {\n      return 100;\n    }\n    return Math.min(this.upperLimitPercent, Math.max(0, this.collectedPercent));\n  }\n\n  calculateAngle(): string {\n    const percent = this.calculatePercent();\n    return `${percent * 3.6}deg`;\n  }\n  \n  render() {\n    return (\n      <Host> \n        <div style={{'--collected-angle': this.calculateAngle() }} class=\"station-icon-wrapper\">\n          <div class=\"station-icon\">\n            <slot />\n            { (this.collected === true || this.collectedPercent >= this.upperLimitPercent) &&\n              <div class=\"collected-icon animate-pop-appearance\">\n                <ion-icon name=\"checkmark\"></ion-icon>\n              </div>\n            }\n          </div>\n        </div>\n      </Host>\n    );\n  }\n}"],"mappings":"kDAAA,MAAMA,EAAiB,kuC,MCOVC,EAAW,MALxB,WAAAC,CAAAC,G,UAOUC,KAAiBC,kBAAW,GAK5BD,KAASE,UAAY,MAKrBF,KAAgBG,iBAAW,CA8BpC,CA5BC,gBAAAC,GACE,GAAIJ,KAAKG,kBAAoBH,KAAKC,kBAAmB,CACnD,OAAO,G,CAET,OAAOI,KAAKC,IAAIN,KAAKC,kBAAmBI,KAAKE,IAAI,EAAGP,KAAKG,kB,CAG3D,cAAAK,GACE,MAAMC,EAAUT,KAAKI,mBACrB,MAAO,GAAGK,EAAU,Q,CAGtB,MAAAC,GACE,OACEC,EAACC,EAAI,CAAAC,IAAA,4CACHF,EAAA,OAAAE,IAAA,2CAAKC,MAAO,CAAC,oBAAqBd,KAAKQ,kBAAoBO,MAAM,wBAC/DJ,EAAK,OAAAE,IAAA,2CAAAE,MAAM,gBACTJ,EAAQ,QAAAE,IAAA,8CACLb,KAAKE,YAAc,MAAQF,KAAKG,kBAAoBH,KAAKC,oBAC1DU,EAAK,OAAAE,IAAA,2CAAAE,MAAM,yCACTJ,EAAU,YAAAE,IAAA,2CAAAG,KAAK,iB","ignoreList":[]}